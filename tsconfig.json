{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "noImplicitAny": true,
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist/",
    "removeComments": true,
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "paths": {
      "utils": [
        "utils"
      ],
      "components": [
        "components"
      ],
      "layouts": [
        "layouts"
      ],
      "routes": [
        "routes.ts"
      ],
      "StateProvider": [
        "utils/StateProvider.tsx"
      ],
      "ApiProvider": [
        "utils/ApiProvider.ts"
      ],
      "Theme": [
        "App.theme.ts"
      ],
      "assets": [
        "assets"
      ],
      "typings": [
        "typings"
      ]
    }
  }
}